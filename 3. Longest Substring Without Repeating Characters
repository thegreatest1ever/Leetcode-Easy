/**
 * @param {string} s
 * @return {number}
 */
//var lengthOfLongestSubstring = function(s) {
//    
//};
function lengthOfLongestSubstring(s) {
  // Create a set to store the seen characters.
  const seen = new Set();

  // Initialize the start and end indexes.
  let start = 0;
  let end = 0;

  // Initialize the maximum length.
  let maxLen = 0;

  // Iterate through the string.
  for (let i = 0; i < s.length; i++) {
    // If the character is not seen yet, add it to the set and update the end index.
    if (!seen.has(s[i])) {
      seen.add(s[i]);
      end++;
    } else {
      // If the character is seen, remove the first character from the set and update the start index.
      seen.delete(s[start]);
      start++;
    }

    // Update the maximum length.
    maxLen = Math.max(maxLen, end - start);
  }

  // Return the maximum length.
  return maxLen;
}
